.breadcrumb
	background-color transparent
	padding-top 50px
	padding-bottom 50px
	.breadcrumb-item
		padding-left 0
		align-items center
		a
			font-size 15px
			color #292929
			line-height 1
			.fa
				display inline-block
				vertical-align middle
				margin-left 10px
				margin-right 10px
	.breadcrumb-item+.breadcrumb-item::before
		display none