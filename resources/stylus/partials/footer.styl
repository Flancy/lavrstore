.footer
	padding-top 70px
	padding-bottom 30px
	.footer__item
		flex 0 0 15%
		max-width 15%
		text-align center
		div
			display inline-block
			text-align left
	.footer__brand
		text-align center
		img
			margin-top 40px
			display inline-block
			width 96px
			height auto
	.text-head
		color #797979
		font-size 15px
		font-weight bold
		line-height 1
		text-transform uppercase
		margin-bottom 25px
		text-align left
	.footer__link
		display block
		color #292929
		font-size 15px
		font-weight bold
		line-height 1
		margin-bottom 10px
		text-align left
	.footer__link-social
		width 22px
		height 22px
		display inline-block
		vertical-align middle
		margin 0 10px

.developed
	padding-bottom 20px
	padding-right 10px
	.developed__link
		display inline-block
		font-size 12px
		color #AAAAAA